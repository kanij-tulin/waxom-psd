#service {
	padding: 50px 0;
}
.container{
    width:1170px;
    margin:0 auto;
  
}
.service_container .ser_item{
    width:280px;
    text-align:center;
    padding-top:0 15px;
    position: relative;
    padding-top:50px;
}
.service_container .ser_item h4{
    font-size:22px;
    font-family: 'Raleway', sans-serif;
    padding-top:25px;
    color:#747474;
    transition:all-linear 5s;

}
.service_container .ser_item p{
    font-size:14px;
    font-family: 'Raleway', sans-serif;
    line-height: 24px;
    padding-top:14px;
    padding-bottom:25px;
    color:#b7b7b7;
     position: relative; 

}
.service_container .ser_item img {
	position: absolute;
	left: 46%;
	top: 7px;
	transform: translate x(-50%);
	transform: translateX;
	transform: translatex(-50%);
    transition:all-linear .5s;
}
.service_container .ser_item img:nth-child(2){
    opacity:0;
}
.service_container .ser_item:hover img:nth-child(2){
    opacity:1;
}
.service_container .ser_item:hover img:nth-child(1){
    opacity:0;
}
.service_container .ser_item:hover h4{
   color:#c7b299;
}
.service_container .ser_item p:after{
    position:absolute;
    content:"";
    width:70px;
    height:3px;
    background:#f1eee9;
    left:50%;
    transform:translatex(-50%);
    bottom:0;
    transition:all-linear 5s;
}
.service_container .ser_item:hover p:after{
 background:#c7b299;
}
