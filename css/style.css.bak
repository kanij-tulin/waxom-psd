 /* project start*/
    .pro_text{
     width:800px;
    margin:0 auto;
    padding:40px 0px;
    text-align:center;
    } 
    .pro_text h4{
        font-size:30px;
        font-family: 'Raleway', sans-serif; 
        font-weight:600;
    }
    .pro_text p{
        font-size:16px;
         padding-top:25px;
         font-family: 'Montserrat', sans-serif;
          line-height: 25px;
    }
    .pro_btn{
        font-size: 14px;
        font-family: 'Montserrat', sans-serif;
        font-weight:600;
        text-align:center;
       
    }
    .pro_btn button{
        padding:14px;
        border:1px solid #615855;
        border-radius:3px;
         background: transparent;  
        font-family: 'Montserrat', sans-serif;
        font-weight:200;
        margin-right:10px;
       
    }
 .pro_btn button:hover {
        background:#998675;
        color:#fbfaf8;
    }
    .pro_it{
        display: flex;
        gap:30px;
        flex-wrap:wrap;
    }
    .pro_it .items{
        width: 370px;
       padding:30px 0;
       background: #fbfaf8;

    }
    .pro_it .image img{
        max-width:100%;
        display: block;
        

      

    }
    .pro_it .image{
        position: relative;
    } 
    .pro_it .items .overlay{
        position: absolute;
         top:0px;
        left:0px; 
        width:100%;
        height:100%; 
      background:rgba(17, 17, 17, 0.70);
      display: flex;
     align-items: center ;
     justify-content: center;
     gap:25px;
     transform: scale(0);
     
     transition: all linear 4s ;
    }
    .pro_it .items:hover .overlay{
     transform: scale(1);
} 
    .pro_it .image .overlay a{
        color:#fff;
    }
    /* .pro_it .items:hover img{
        transform:  scale(1) rotate(5deg);
    } */
    .pro_info h4{
        font-size:20px;
        font-family: 'Raleway', sans-serif; 
        font-weight:400;
       padding-top:25px;
       color:#998675;
    }
  .pro_it .pro_info p{
    font-size:14px;
    font-family: 'Montserrat', sans-serif;
    font-weight:300;
    padding-top:10px;}

 .pro_it .pro_info {
     position:relative;
     transition: all linear 4s;
 }
  .pro_it .pro_info:after{
     position:absolute;
     content:"";
     width:30px;
     height:30px;
     background:#fff;
     transform: rotate(45deg);
     left:30px;
     top:-15px;
     z-index:1; 
     transition: all linear 4s;
 }
 .pro_it .pro_info h4{
     color:#c7b299;
     transition: all linear 4s;
     
 }
 .pro_it .pro_info p{
    transition:  all linear 4s;
 }
 
.pro_it .items:hover .pro_info{
    background:#362f2d;
 
}

.pro_it .items:hover .pro_info:after{
    background: #362f2d;
}


